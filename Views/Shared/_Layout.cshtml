<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/NAUT.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/DataTables.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/nivo-slider.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/default.css")" rel="stylesheet" type="text/css" />

@*    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script  src="@Url.Content("~/Scripts/DataTables.js")" type="text/javascript"></script>
    <script  src="@Url.Content("~/Scripts/jquery.nivo.slider.pack.js")" type="text/javascript"></script>
@*    <script  src="@Url.Content("~/Scripts/jquery.nivo.slider.js")" type="text/javascript"></script>*@

</head>

<body>
    <div class="page">
        <div id = "header">
            <div id= "title">
               @* <h1>Nepalese Association of UTAH</h1>*@
                @*<img alt = "NAUTBanner" src="../../Content/Images/NAUTBanner.jpg" style="width:100%; border:2px solid white;"/>*@
                <img alt = "NAUTBanner" src="../../Content/Images/NAUTBannerNew.jpg" style="width:100%; border:1px solid gray;"/>
            </div> 
            
            <div id="logindisplay" style="margin-bottom:-20px">
               @* @Html.Partial("_LogOnPartial")*@
                <a href="http://www.youtube.com/user/NepalUtah"> <img alt = "Youtube" src="../../Content/Images/youtube.png"/></a>
                <a href="https://www.facebook.com/utahnepal"> <img alt = "Facebook" src="../../Content/Images/facebook.png" /></a>
            </div>
                <ul class="NAUTMenu">
                    <li>@Html.ActionLink("Home", "Home", "Home")</li>
                    <li class ='About'>@Html.ActionLink("About", "About", "About")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Contact")</li>
                    <li>@Html.ActionLink("Events", "Events", "Events")</li>
                    <li>@Html.ActionLink("Members", "Members", "Members")</li>
                    <li>@Html.ActionLink("Directory", "Directory", "Directory")</li>
                    <li>@Html.ActionLink("Documents", "Documents", "Documents")</li>
                </ul>
           </div>

        <div id="main">
            @RenderBody()
        </div>
        <div class = "Sponsors">
            <a href="http://bigdaddyspizza.com/">
                <img alt = "BigDaddy" src="../../Content/Images/big_daddy_anim.gif" style="width:193px; height:100px; border:1px solid gray; margin-right:4px"/>
            </a>
        </div>
        <div class = "Sponsors">
                <img alt = "BigKahuna" src="../../Content/Images/big_kahuna_anim.gif" style="width:192px; height:100px; border:1px solid gray; margin-right:4px"/>
        </div>
        <div class = "Sponsors">
            <a href="http://www.browspa24.com/">
                <img alt = "EyeBrow" src="../../Content/Images/eyebrowspa_logo.jpeg" style="width:192px; height:100px; border:1px solid gray; margin-right:4px"/>
            </a>
        </div>
        <div class = "Sponsors">
            <img alt = "ShopAndGo" src="../../Content/Images/shopngo.jpeg" style="width:192px; height:100px; border:1px solid gray; margin-right:4px;"/> 
        </div>
        <div class = "Sponsors" style="float:right">
            <a href="http://www.himalayankitchen.com/">
                <img alt = "HimKitchen" src="../../Content/Images/Himalayan_kitchen_logo.jpeg" style="width:193px; height:100px; border:1px solid gray;float:right"/>
             </a>
        </div>
        <br />
        <div id = "footer">
            (c) Nepalese Association of Utah. Post Box 510938, Salt Lake City, Utah 84151
        </div>
    </div>
</body>
</html>
